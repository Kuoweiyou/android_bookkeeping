<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.BookKeeping" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#F1EFDE</item>
        <item name="colorPrimaryVariant">?attr/colorPrimary</item>
        <item name="colorOnPrimary">#1C1C04</item>
        <item name="colorAccent">#FCDCBC</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#EBE7D7</item>
        <item name="colorSecondaryVariant">?attr/colorSecondary</item>
        <item name="colorOnSecondary">#FCFCF4</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="colorSurface">#ffffff</item>
    </style>

    <style name="ThemeOverlay.Self.Dark.ActionBar" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="colorControlNormal">?attr/colorOnPrimary</item>
    </style>

    <style name="ThemeOverlay.Self.Light" parent="@style/ThemeOverlay.Material3.Light" />
</resources>

